{
  "name": "quiet-quitting",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .ts src/**.ts",
    "format": "prettier --write --parser typescript \"src/**/*.ts?(x)\"",
    "prepare": "husky install",
    "start": "nodemon",
    "test": "jest"
  },
  "keywords": [],
  "author": "Coda",
  "license": "MIT",
  "nodemonConfig": {
    "watch": [
      "src/**"
    ],
    "ext": "ts",
    "ignore": [
      "src/**/*.{spec,test}.ts"
    ],
    "exec": "NODE_ENV=development ts-node-esm src/index.ts"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "eslint",
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@swc/core": "^1.3.62",
    "@swc/helpers": "^0.5.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "regenerator-runtime": "^0.13.11",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}